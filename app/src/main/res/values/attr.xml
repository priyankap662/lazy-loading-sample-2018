<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FixedRatioImageView">
        <attr name="widthValue" format="integer" />
        <attr name="heightValue" format="integer" />
        <attr name="aspectRatioEnabled" format="boolean" />
        <attr name="dominantMeasurement">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>

</resources>